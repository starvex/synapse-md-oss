{
  "name": "@synapse-md/client",
  "version": "0.1.0",
  "description": "TypeScript client SDK for the Synapse protocol â€” connect any agent in 3 lines",
  "author": "Synapse.md",
  "license": "MIT",
  "keywords": [
    "synapse",
    "agent",
    "ai",
    "multi-agent",
    "protocol",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/synapse-md/synapse-md-oss"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && node -e \"require('fs').writeFileSync('dist/cjs/package.json', JSON.stringify({type:'commonjs'}))\"",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {}
}
